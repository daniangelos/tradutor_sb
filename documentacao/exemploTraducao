section text
add n1
sub n2
load n3 + 1
output n3
stop


section data
n1: space
n2: space
n3: space 4



->

section .text
add eax, [n1]
sub eax, [n2]
mov eax, [n3 + 1]
mov ecx, [n3]
call output
mov eax,1
mov ebx,0
int 0x80

section .bss
n1: resw 1
n2: resw 1
n3: resw 4





output:
	enter 0,0
	->
	push ebp
	mov ebp, esp
	pusha
	
	
	mov eax,4
	mov ebx,1
	add ecx,0x30
	mov edx,1

	popa
	leave
	ret

